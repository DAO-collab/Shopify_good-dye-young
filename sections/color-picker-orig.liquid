<script type="text/javascript" ecl="true">jQuery(document).ready(function($){

	function colorPickerMobile() {

		$('.color-items .color-item').click(function(){

			if($(this).hasClass('active')) {

				$('.color-item').removeClass('active');
				$(this).removeClass('active');
				$(this).closest('.color-picker-wrap').find('.color-product-block .product-default').show();
				$(this).closest('.color-picker-wrap').find('.color-product-block .product-hover').hide();

				$('.color-product-item').hide();

			} else {
				$('.color-item').removeClass('active');
				$(this).addClass('active');
				$(this).closest('.color-picker-wrap').find('.color-product-block .product-default').hide();
				$(this).closest('.color-picker-wrap').find('.color-product-block .product-hover').show();

				var product_id = '#' + $(this).attr('data-product');
				$(this).closest('.color-picker-wrap').find('.color-product-items').find(product_id).show();
			}

		});
	}

	if ($(window).width() <= 767) {
		colorPickerMobile();
	}
	
	$(window).on('resize', function() {
		if ($(window).width() <= 767) {
			colorPickerMobile();
		}
	});
	
});</script>
<script type="text/javascript" ecl="true">jQuery(document).ready(function($){

	function colorPicker() {
		$('.color-items .color-item').mouseover(function() {
			$(this).closest('.color-picker-row').find('.product-default').hide();
			$(this).closest('.color-picker-row').find('.product-hover').show();

			var product_id = '#' + $(this).attr('data-product');
			$('.color-product-item').hide();
			$(this).closest('.color-picker-row').find('.color-product-items').find(product_id).show();
		})
		
		$('.color-items .color-item').mouseout(function(){

			if ($('.color-item').hasClass('active')){
				console.log('true');

				var product_id = '#' + $('.color-item.active').attr('data-product');
				console.log(product_id);

				$('.color-product-item').hide();
				$('.color-product-items').find(product_id).show();
			}
			else {
				if(!$(this).hasClass('active')) {
					var product_id = '#' + $('.color-item.active').attr('data-product');
					$(this).closest('.color-picker-row').find('.product-default').show();
					$(this).closest('.color-picker-row').find('.product-hover').hide();
					$(this).closest('.color-picker-row').find('.color-product-items').find(product_id).hide();
				}
			}                    

		});

		$('.color-items .color-item').click(function(){

			if($(this).hasClass('active')){
				$('.color-item').removeClass('active');
				$(this).removeClass('active');
				$(this).closest('.color-picker-row').find('.product-default').show();
				$(this).closest('.color-picker-row').find('.product-hover').hide();
				$('.color-product-item').hide();
			}
			else {
				$('.color-item').removeClass('active');
				$(this).addClass('active');
				$(this).closest('.color-picker-row').find('.product-default').hide();
				$(this).closest('.color-picker-row').find('.product-hover').show();

				var product_id = '#' + $(this).attr('data-product');
				$(this).closest('.color-picker-row').find('.color-product-items').find(product_id).show();
			}
			
		});
	}

	if ($(window).width() >= 768) {
		colorPicker();
	}
	
	$(window).on('resize', function() {
		if ($(window).width() >= 768) {
			colorPicker();
		}
	});
});</script>

